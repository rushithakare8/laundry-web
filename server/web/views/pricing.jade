extends home/layout

block styles

block content
  #page-wrapper.page-wrapper
    include home/menu
    .page-title.callout.large.primary
      .row.column.text-center
        .columns.small-12
          img(src='/public/img/logo-sm.png')
        .columns.small-12(class='Pt(32px)')
          h1.shadow Precio de servicios. 
    #shorts.shorts
      .row(class='Py(40px)')
        .columns.medium-8.medium-offset-2.small-12
          .row.small-up-2.large-up-3
            .column
              ul.pricing-table.no-bullet.text-center
                li.title 
                  .circ-step(class='My(36px)')
                    img(src='/public/img/washing-machine.svg', width='36px', height='36px')
                li.price Desde $80.00
                li Lavanderia
                li.description Servicio de lavado desde $80, compara nuestros precios!
                li Entrega programada
                li
                  a.button(href='#' data-open="washingPriceDetails" ) Informacion
            .column
              ul.pricing-table.no-bullet.text-center
                li.title 
                  .circ-step(class='My(36px)')
                    img(src='/public/img/plancha.svg', width='36px', height='36px')
                li.price Desde $70.00
                li Planchaduria
                li.description Servicio de planchado desde $70, compara nuestros precios!
                li Entrega programada
                li
                  a.button(href='#' data-open="ironingPriceDetails" ) Informacion
            .column
              ul.pricing-table.no-bullet.text-center
                li.title 
                  .circ-step(class='My(36px)')
                    img(src='/public/img/gancho.svg', width='36px', height='36px')
                li.price Desde $38.00
                li Tintoreria
                li.description Servicio de tintoreria desde $38, compara nuestros precios!
                li Entrega programada
                li
                  a.button(href='#' data-open="dryCleaningPriceDetails" ) Informacion
