{
  "rules": {
    "max-len": 0,
    "react/no-array-index-key": 1,
    "react/forbid-prop-types": [0, { "forbid": ["any", "object"]}],
    "jsx-a11y/no-static-element-interactions": 1
  },
  "extends": "airbnb",
  "plugins": [
    "react",
    "babel"
  ],
  "env": {
    "browser": true,
    "node": true
  }
}
